<ipayMsg client="<%= @params[:client_id] %>" term="<%= @params[:term] %>" seqNum="2" time='<%= @params[:time] %>'>  <elecMsg ver="2.37">    <vendRevReq <%= if @params[:rep_count] > 0 then "RepCount='#{@params[:rep_count]}'" end %> origTime='<%= @params[:orig_time] %>'>      <ref><%=@params[:reference] %></ref>      <vendReq>        <ref><%= @params[:orig_reference] %></ref>        <amt cur='<%= @params[:currency] %>'><%= @params[:amount] %></amt>        <numTokens><%= @params[:num_tokens] %></numTokens>        <meter><%= @params[:meter] %></meter>        <payType><%= @params[:pay_type] %></payType>      </vendReq>    </vendRevReq>  </elecMsg></ipayMsg>