<ipayMsg client="<%= @params[:client] %>" term="<%= @params[:term] %>" seqNum="<%= @params[:seq_num] %>" time='<%= @params[:time] %>'>  <elecMsg ver="2.37">    <vendRevReq <%= if @params[:rep_count] > 0 then "RepCount='#{@params[:rep_count]}'" end %> <%= if @params[:orig_time] then "origTime='#{@params[:orig_time]}'" end %> >      <ref><%=@params[:ref] %></ref>      <vendReq>        <ref><%= @params[:orig_reference] %></ref>        <amt cur='<%= @params[:currency] %>'><%= @params[:amount] %></amt>        <numTokens><%= @params[:num_tokens] %></numTokens>        <meter><%= @params[:meter] %></meter>        <payType><%= @params[:pay_type] %></payType>      </vendReq>    </vendRevReq>  </elecMsg></ipayMsg>